---
import type { Link } from "@/data/resume-data";
import Reactor from "./Reactor";

type Props = {
    link: Link;
};

const { link } = Astro.props as Props;
---

<div class="link">
    {link.icon && <Reactor component={link.icon} />}
    <a href={link.url}>{link.prettyUrl || link.url}</a>
</div>
